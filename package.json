{
  "name": "jspet",
  "version": "0.0.1",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint src test",
    "coverage": "istanbul cover _mocha -- --recursive test",
    "preverify": "npm run lint && npm run coverage",
    "verify": "istanbul check-coverage",
    "test": "mocha --recursive test",
    "watch": "npm test -- --watch --growl --reporter min"
  },
  "author": "Pavel Vetokhin",
  "license": "MIT",
  "dependencies": {
    "asyncawait": "1.0.4",
    "mathjs": "3.2.1",
    "mongoose": "4.5.3"
  },
  "devDependencies": {
    "babel-eslint": "6.1.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "dirty-chai": "1.2.2",
    "eslint": "3.0.0",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-import": "1.10.0",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "script"
    },
    "extends": "airbnb-base",
    "env": {
      "node": true,
      "mocha": true
    }
  }
}
